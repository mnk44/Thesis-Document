@Article{gestorBases,
  author    = {Gil Cruz Lemus and Raisa Socorro Llanes and Ana Mailen Rodríguez Riverón},
  journal   = {Revista Cubana de Ingeniería},
  title     = {Sistema para gestionar bases de conocimiento en la industria alimentaria},
  year      = {2018},
  issn      = {2223 - 1781},
  month     = may,
  number    = {2},
  pages     = {60 - 68},
  volume    = {IX},
  abstract  = {En la actualidad, el mercado diverso y altamente competitivo exige a las empresas almacenar y analizar una gran diversidad de información, los Sistemas Expertos (SE) se destacan por herramientas de soporte para la toma de decisiones. El presente trabajo desarrolló un sistema para gestionar bases de conocimiento en la Industria Alimentaria, permitiendo la extracción del conocimiento al experto del proceso, la elaboración de las bases de conocimientos necesarias y la modificación de las mismas. Estas bases se utilizan para el trabajo del sistema experto y para el entrenamiento de operarios en procesos tecnológicos en la industria alimentaria mostrando las posibles causas de los problemas, así como las recomendaciones necesarias para eliminarlos. El sistema permite el análisis de la base de conocimiento a partir de los datos suministrados por el usuario, se realiza la inferencia y muestra los resultados. Utiliza las reglas de producción como formalismo de producción y el Drools conocido como JBoss Rules como motor de inferencia. La herramienta obtenida agiliza el proceso de adquisición y representación del conocimiento, y permite al especialista del proceso crear las bases sin la intervención directa de un ingeniero en conocimiento y permite almacenar el mismo para ser utilizado por los nuevos especialistas. Se crearon las bases de conocimiento de dos procesos de la industria alimentaria analizados: el de elaboración de néctar de naranja mandarina y el de elaboración de queso fresco por vía enzimática. Los ficheros generados por el sistema fueron importados por el sistema experto y por el de entrenamiento de forma satisfactoria y las respuestas obtenidas coinciden con el árbol de inferencia original del que se partió para elaborar las bases de conocimientos.},
  booktitle = {Revista Cubana de Ingeniería},
  comment   = {Se enfoca en la problemática que se desea resolver en la investigación que estoy realizando. Plantea información sobre la Industria Alimentaria y el Sistema Gestor de Bases de Conocimiento que brinda la información al nuevo sistema que se debe desarrollar. Este articulo me sirve de referencia para la introduccion y el desarrollo del capitulo 1},
  file      = {:Sistemas Expertos\\1621.pdf:PDF},
  keywords  = {sistema experto, inteligencia artificial, reglas de producción, motores de reglas, base de conocimiento, adquisición del conocimiento},
  publisher = {Revista Cubana de Ingeniería},
}

@PhdThesis{avanceTecnologico,
  author   = {Ángel Alegre Burcio},
  school   = {E.T.S.I. Industriales (UPM)},
  title    = {Implementación de estrategias de Computation Offloading en redes de sensores inalámbricas de bajo consumo para Internet de las Cosas},
  year     = {2022},
  address  = {Biblioteca ETSI Industriales},
  month    = feb,
  type     = {resreport},
  abstract = {En la actualidad, las comunicaciones inalámbricas y la inmediatez de éstas se han convertido en prácticamente una obligación en la mayoría de los entornos laborales, industriales y sociales. En un mundo tan globalizado y con una tecnología que avanza y se supera con una rapidez vertiginosa, no resulta extraño que cada vez se exijan mejores condiciones y prestaciones a la hora de entablar comunicaciones inmediatas y de calidad. A partir de estas exigencias surge la necesidad de establecer un entorno que permita desarrollar comunicaciones de manera inmediata, y más importante, de manera automática, para facilitar todo tipo de actividades, desde lo laboral y comercial a lo cotidiano. En esta situación nace el paradigma del Internet de las Cosas (Internet of Things), cuya importancia hoy en día trasciende todo tipo de fronteras: desde simples comunicaciones a través de redes sociales, pasando por almacenamiento y procesamiento de datos, hasta reconocimiento de imágenes o vídeo, entre otros. Este sistema se basa en la interconexión de múltiples redes inalámbricas descentralizadas (Wireless Sensor Networks, WSN), que a su vez están formadas por una infinidad de dispositivos interconectados entre sí, también denominados “nodos”, con la capacidad de medir parámetros mediante sensores, comunicar la información obtenida y procesarla hasta cierto punto. El Computation Offloading aparece como complemento a este paradigma, permitiendo aumentar el rendimiento de los sistemas mediante la comunicación y compartición de trabajo de procesamiento entre diferentes dispositivos, con distintas capacidades técnicas y de computación. De esta manera, sensores de baja capacidad de cálculo o de procesamiento pueden enviar los datos recopilados a dispositivos con mayores prestaciones a través de la nube (cloud), para después recibir los resultados obtenidos por estos últimos. Sin embargo, con el paso del tiempo, la cantidad de datos transmitidos y de canales de comunicación necesarios para esto se vuelve cada vez mayor, lo que provoca que la nube quede obsoleta en muchas situaciones, perdiendo prestaciones debido a problemas de capacidad de almacenamiento, latencia o ancho de banda. Una manera de resolver esta situación surge con el denominado “Edge Computing” (o “Fog Computing”), que permite la “descentralización del IoT”, haciendo que la nube no sea necesariamente el destino final donde se procese la información. De esta manera, se crea una serie de subestaciones, más cercanas a los dispositivos nodos, denominados “dispositivos de borde” o “Edge devices”, de manera que la información se procese en estos como punto intermedio entre sensor y nube, sólo requiriendo esta última en casos concretos, y pudiendo subdividirse la información para trabajarse a diferentes niveles de jerarquía según la capacidad de procesamiento. Ambos conceptos suponen un avance enorme para el IoT, dando lugar a la creación y desarrollo de nuevas técnicas de comunicación, sistemas y dispositivos, gracias a la capacidad para poder delegar parte del trabajo a los dispositivos de borde, de manera más rápida que con la nube. Aun siendo este sistema una mejora considerable, debe enfrentarse a diferentes tipos de limitaciones, sobre todo relacionadas con la portabilidad y bajo coste de los dispositivos, generalmente de baja potencia y batería, que dan lugar a restricciones en cuanto a ancho de banda, capacidad de procesamiento, velocidad de comunicación, etc. Para el desarrollo de este proyecto, la restricción de mayor interés es la de la baja capacidad energética de las baterías, aunque también se tienen en cuenta otras como el ancho de banda disponible entre dispositivos y su propia capacidad de procesamiento o cómputo de los datos. La transmisión de datos de los dispositivos sensores consume más energía que su procesamiento. Por este motivo, se deben desarrollar estrategias o algoritmos que permitan, en función del nivel de batería, decidir si es óptimo procesar los datos localmente o transmitirlos a otro dispositivo. Estas estrategias variarán en función de cada tipo de red, dispositivo, objetivo y expectativas de rendimiento y calidad de servicio de estos. En el caso de estudio de este trabajo, se utilizarán algoritmos metaheurísticos que buscarán maximizar la vida útil de cada uno de los nodos de la red, desarrollando la capacidad de seleccionar aquellos más capaces de procesar cada una de las tareas existentes. En cada momento, los algoritmos seguirán examinando las capacidades de cada nodo, de manera que, si uno de ellos sin ocupación posee mejores prestaciones en un determinado momento, pueda sustituir a un nodo que se encuentre procesando una tarea y que cuente con poca energía. Además, se desarrollarán dos modelos diferentes, teniendo el primero sólo en cuenta los niveles energéticos de cada nodo, mientras que el segundo incluirá adicionalmente como parámetro de decisión el ancho de banda disponible en cada uno de estos, aumentando la complejidad y profundidad del problema. Así, el algoritmo debe ser capaz de contemplar el mejor resultado de entre los posibles, decidiendo qué parte de la información se procesa en cada dispositivo, y qué dispositivo se elige para transmitirle los datos que requieran mayor capacidad de cómputo, mediante metodologías conocidas como “Resource Allocation” (siendo esta una de las partes más difíciles de desarrollar de todo el conjunto). El funcionamiento básico de la red consistirá en el envío de parámetros desde cada uno de los nodos al nodo servidor o clúster, que almacenará la información para compartirla con los algoritmos. Estos, a su vez, procesan dicha información y envían los resultados de vuelta al servidor, donde se comprueba la disponibilidad de los nodos seleccionados por el algoritmo para la asignación del procesamiento de tareas. Además, el nodo servidor gestiona el posible relevo entre los nodos para el procesamiento de las tareas, si sus características así lo requieren.},
  comment  = {Es importante porque hace un análisis del avance de la tecnología en nuestros tiempos. Me sirve de referencia para la introduccion del tema de tesis.},
  url      = {https://oa.upm.es/69691/},
}

@Article{industriaCubana,
  author   = {Antonio Santamaría García},
  journal  = {Consejo Superior de Investigaciones Científicas (España)},
  title    = {Trabajo en la agroindustria cañera cubana, 1898-1914: estrategias para aumentar la producción con escasez de mano de obra},
  year     = {2022},
  issn     = {1139-1472},
  month    = apr,
  number   = {86},
  pages    = {171-206},
  volume   = {3},
  abstract = {La agroindustria azucarera en Cuba experimentó un fuerte crecimiento de su oferta tras recuperarse de la guerra de independencia (1885-1898) y antes de la Primera Guerra Mundial. Ese incremento se concentró en el cuatrienio 1910-1914 debido a que transformaciones anteriores del sector permitieron responder a las nuevas condiciones de la demanda que se presentaron entonces. La historiografía ha estudiado algunos de los factores que explican dicha trasformación, pero no el papel del trabajo, tradicionalmente escaso en la isla. Este artículo analiza y cuantifica cómo se consiguió la mano de obra necesaria, y argumenta que fue debido a la combinación de múltiples variables. Por un lado, se sabe que había en el país más población que la censada. Por otro, las medidas sanitarias aplicadas, junto a los cambios tecnológico-organizativos en los centrales y la inmigración incrementaron la cantidad y capacidad por persona de la fuerza laboral. También ayudaron los salarios altos y el traspaso de la parte agraria de la producción a familias de colonos. Pero aun así el volumen de operarios requerido solo pudo obtenerse con una articulación del conjunto de la economía en torno a la industria azucarera y con una movilización de trabajadores que, al final de la década de 1910, fue posible por la extensión de la red de ferrocarriles.},
  comment  = {Es un ejemplo del avance de la tecnología en Cuba muy relacionado con el trabajo de investigación porque el objetivo principal del artículo es la Industria Alimentaria en el sector azucarero. Me sirve tanto para la introduccion como para el capitulo 1},
  file     = {:Avances en la industria cubana\\Trabajo en la agroindustria cañera cubana.pdf:PDF},
  keywords = {Cuba, industria azucarera, trabajo, demografía},
  url      = {http://hdl.handle.net/10261/265946},
}

@Article{invstCibernCuba,
  author   = {Ileana Dopico Mateo and Armando Plasencia Salgueiro},
  journal  = {Revista Control, Cibernética y Automatización},
  title    = {Vinculación de la formación doctoral con los Programas Nacionales de Investigación en Cuba. La experiencia del Doctorado en Cibernética Aplicada del Instituto de Cibernética, Matemática y Física (ICIMAF)},
  year     = {2021},
  issn     = {2312-5276},
  month    = jan,
  number   = {1},
  pages    = {61},
  volume   = {9},
  abstract = {La difusión e implementación de los avances tecnológicos en la industria y la economía a nivel global vienen acompañadas de la necesidad de formar factores humanos capaces de responder a las exigencias de la “nueva ola” del desarrollo científico tecnológico en el siglo XXI. Cuba no está ajena a la nueva revolución tecnológica por lo que el país está abocado a la creación y fortalecimiento de programas de posgrado que consoliden, complementen, profundicen y originen nuevos conocimientos de forma novedosa para aquellos profesionales inmersos en áreas relacionadas con estos cambios de la era digital, proporcionándoles herramientas tecnológicas para afrontar retos, tomar decisiones y solucionar problemas de la sociedad cubana en general con una alta profesionalidad y basamento científico. El trabajo que se pone a consideración tiene como objetivo mostrar la experiencia de la formación en el marco del Programa doctoral en Cibernética Aplicada del Instituto de Cibernética, Matemática y Física  (ICIMAF) con su vinculación con las proyecciones del Programa Nacional en Automática, Robótica e Inteligencia Artificial (ARIA), auspiciado por el CITMA, en respuesta a las nuevas políticas de desarrollo y crecimiento económico que propugna el gobierno cubano.},
  comment  = {En el artículo se mencionan avances tecnológicos cubanos y profundizan en el área de la cibernética. Esta investigación sirve como base de modelo tecnológico en la parte de los avances en nuestro país. Me sirve para la introduccion de mi tema de tesis},
  file     = {:Avances en la industria cubana\\Vol9No12021.pdf:PDF},
  keywords = {avances tecnológicos; enseñanza por proyectos; formación doctoral; programas nacionales},
  url      = {https://www.researchgate.net/profile/Ramon-Lopez-Suarez/publication/359222139_Sistema_de_bajo_costo_para_monitoreo_del_consumo_de_energia_electrica_en_tiempo_real_Revista_Control_Cibernetica_y_Automatizacion_ISSN_2312-5276_Vol_9_No_1_enero_-_junio_2021/links/622fa6c3435ec5645a11f96a/Sistema-de-bajo-costo-para-monitoreo-del-consumo-de-energia-electrica-en-tiempo-real-Revista-Control-Cibernetica-y-Automatizacion-ISSN-2312-5276-Vol-9-No-1-enero-junio-2021.pdf#page=61},
}

@Article{industriaFarmceutica,
  author   = {Fernando Suárez Concepción and Randy Piñero Aguilar and Alberto Prieto Moreno and Andreis Alfonso Cordoví and Julio Carbó Castro and Orestes Llanes Santiago},
  journal  = {Ingeniería Industrial},
  title    = {Metodología para la automatización de procesos tecnológicos en la industria farmacéutica cubana},
  year     = {2022},
  issn     = {1815-5936},
  month    = feb,
  number   = {1},
  pages    = {179-230},
  volume   = {43},
  abstract = {En este trabajo se presenta una metodología para el diseño, implementación y puesta en marcha de productos de automatización para procesos tecnológicos de la industria farmacéutica cubana basada en un enfoque scrum. El empleo de tecnologías de software propias que reducen el tiempo de desarrollo y puesta marcha, aportan gran flexibilidad y eficiencia en las operaciones, mejoran significativamente la trazabilidad de los procesos y facilitan el trabajo a partir de diseños amigables de la interfase hombre-máquina entre sus principales ventajas. La metodología desarrollada es aplicable a la automatización de la mayoría de los procesos de la industria farmacéutica y garantiza el cumplimiento de las exigentes regulaciones vigentes para esta industria y las establecidas por el Centro de Control de Medicamentos (CECMED) de Cuba. La aplicación de la metodología en la industria farmacéutica cubana ha significado un ahorro al país superior a tres millones y medio en moneda convertible en los últimos cinco años.},
  comment  = {Es un artículo de la CUJAE sobre avances tecnológicos en la industia farmacéutica cubana, un ejemplo de software que ayuda a obtener mejores resultados en la producción. Me sirve de referencia para la introduccion de mi tema de tesis},
  keywords = {enfoque scrum; industria farmacéutica; procesos tecnológicos; tecnologías de software propias; automatización},
  url      = {http://scielo.sld.cu/scielo.php?script=sci_arttext&pid=S1815-59362022000100082},
}

@PhdThesis{elena,
  author   = {Elena Acosta Gil, Raisa Socorro Llanes},
  school   = {Universidad Tecnológica de la Habana “José Antonio Echeverría”},
  title    = {SISTEMA PARA EL ENTRENAMIENTO DE OPERARIOS EN PROCESOS TECNOLÓGICOS EN LA INDUSTRIA ALIMENTARIA},
