@PhdThesis{ElenaAcostaGil2018,
  author   = {Elena Acosta Gil, Raisa Socorro Llanes},
  school   = {Universidad Tecnológica de la Habana “José Antonio Echeverría”},
  title    = {Sistema para el entrenamiento de operarios en procesos tecnológicos en la Industria Alimentaria},
  year     = {2018},
  address  = {La Habana, Cuba},
  month    = jun,
  type     = {software},
  abstract = {Las Empresas de Proyectos de la Industria Alimentaria presenta la necesidad de entrenar a operarios de plantas. Las mismas necesitan poder entrenar de forma rápida, pues en la actualidad existen muchas irregularidades a la hora de operar los procesos debido a que los encargados de realizar estos trabajos a veces no tienen el suficiente conocimiento. En el trabajo se presenta un sistema para entrenar a operadores de procesos tecnológicos en la industria alimentaria. El mismo tiene como objetivo evaluar las decisiones que toman los operarios ante diferentes situaciones críticas y brindar al tecnólogo principal de un proceso los resultados de la evaluación y como se van superando los mismos. Dicho sistema utiliza el Drools como motor de inferencia para validar las respuestas emitidas por el usuario. El sistema se probó con los procesos de Elaboración de queso fresco por vía enzimática y Elaboración de néctar naranja-mandarina obteniéndose resultados satisfactorios.},
  comment  = {Es la tesis referente al sistema que estoy trabajando. Muy util durante toda la investigacion},
  file     = {:C\:\\Monik\\Escuela\\Practicas\\Tesis\\Prácticas 2do\\Tesis\\acosta_gil_elena.pdf:PDF},
  keywords = {Industria Alimentaria, operarios, procesos tecnológicos, motor de inferencia},
}

